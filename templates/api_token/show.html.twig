{% extends 'base.html.twig' %}

{% block title %}ApiToken{% endblock %}

{% block body %}
    <h1>ApiToken</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ api_token.id }}</td>
            </tr>
            <tr>
                <th>Token</th>
                <td>{{ api_token.token }}</td>
            </tr>
            <tr>
                <th>ExpirationDate</th>
                <td>{{ api_token.expirationDate ? api_token.expirationDate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_api_token_index') }}">back to list</a>

    <a href="{{ path('app_api_token_edit', {'id': api_token.id}) }}">edit</a>

    {{ include('api_token/_delete_form.html.twig') }}
{% endblock %}
